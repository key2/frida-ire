<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <ItemDefinitionGroup>
    <Link>
      <SubSystem>Console</SubSystem>
      <RandomizedBaseAddress>false</RandomizedBaseAddress>
      <FixedBaseAddress>true</FixedBaseAddress>
    </Link>
  </ItemDefinitionGroup>

  <ItemGroup>
    <CustomBuild Include="winjector-helper.vala">
      <FileType>Document</FileType>
      <Command>"$(SolutionDir)ext\sdk\vala\valac.exe" winjector-helper.vala winjector-error.vala --ccode --directory=$(IntDir) --vapidir="$(SolutionDir)ext\sdk\vala\vapi" --vapidir="$(IntDir)..\zed-winipc-32" --vapidir="$(IntDir)..\zed-winipc-64" --pkg=gio-2.0 --pkg=zed-winipc || exit 1
echo &gt; "$(IntDir)valacode.stamp"</Command>
      <Message>Compiling vala code</Message>
      <Outputs>$(IntDir)valacode.stamp;%(Outputs)</Outputs>
      <AdditionalInputs>$(SolutionDir)ext\sdk\vala\valac.exe;$(ProjectDir)winjector-error.vala;%(AdditionalInputs)</AdditionalInputs>
    </CustomBuild>
  </ItemGroup>

  <ItemGroup>
    <ClInclude Include="winjector-helper.h" />
  </ItemGroup>

  <ItemGroup>
    <ClCompile Include="$(IntDir)winjector-helper.c" />
    <ClCompile Include="$(IntDir)winjector-error.c" />

    <ClCompile Include="winjector-helper-services.c" />
  </ItemGroup>

</Project>
